{
  "name": "ourcord",
  "version": "1.1.8",
  "main": "dist/export.js",
  "repository": "https://github.com/ourpalace/ourcord",
  "author": "ourcord",
  "license": "MIT",
  "types": "dist/export.d.ts",
  "runkitExampleFilename": "./examples/example.js",
  "scripts": {
    "start": "node dist/example.index.js",
    "upgrade": "node packager.js",
    "prepublish": "tsc",
    "docs:gen": "bash docs.sh",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "npm run docs:gen && cd docs && npm run build",
    "docs": "typedoc --out docs src",
    "prepare": "npm run compile",
    "compile": "run-script-os",
    "compile:win32": "DEL /S /F /Q dist >nul 2>&1 && npx tsc",
    "compile:linux:darwin": "rm -rf dist && npx tsc"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "fast-zlib": "^1.0.5",
    "node-fetch": "^2.6.1",
    "pako": "^2.0.2",
    "ws": "^7.4.1"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^14.14.14",
    "@types/node-fetch": "^2.5.7",
    "@types/pako": "^1.0.1",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "eslint-config-google": "^0.14.0",
    "inquirer": "^7.3.3",
    "typedoc": "^0.19.2",
    "typedoc-plugin-markdown": "^3.1.1",
    "typescript": "^4.1.3",
    "run-script-os": "^1.1.3"
  }
}
